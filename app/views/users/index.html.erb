<h1>Welcome to Travel Note!</h1>
<div class="log-in">
  <h3>Login</h3>
  <%= form_tag '/login' do %>

    Email: <%= text_field_tag :email %>
    Password: <%= text_field_tag :password %>
    <%= submit_tag "Submit" %>

  <% end %>

  <h3>Sign Up</h3>
  <%= link_to 'Create Account', sign_up_path %>

</div>
